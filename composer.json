{
    "name": "kalimeromk/filterable",
    "description": "A Laravel package that simplifies dynamic filtering and searching across models and their relationships, eliminating repetitive query code.",
    "type": "package",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Zoran Shefot Bogoevski",
            "email": "zbogoevski@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Kalimeromk\\Filterable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kalimeromk\\Filterable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kalimeromk\\Filterable\\FilterableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "2.0.7"
    },
    "minimum-stability": "dev"
}
