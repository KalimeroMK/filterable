{
    "name": "kalimeromk/filterable",
    "description": "Address commonly face the problem of adding repetitive filtering code",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Zoran Shefot Bogoevski",
            "email": "zbogoevski@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Kalimeromk\\Filterable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kalimeromk\\Filterable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kalimeromk\\Filterable\\FilterableServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
